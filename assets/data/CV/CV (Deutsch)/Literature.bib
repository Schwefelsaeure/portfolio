@Article{aiello-1977,
  Title                    = {Crowding and the Role of Interpersonal Distance Preference},
  Author                   = {John R. Aiello and Donna T. DeRisi and Yakov M. Epstein and Robert A. Karlin},
  Journal                  = {Sociometry},
  Year                     = {1977},

  Month                    = {09},
  Number                   = {3},
  Pages                    = {271--282},
  Volume                   = {40},

  Abstract                 = {An experiment was performed to examine the role of interpersonal distance preference as a mediator of short-term crowding involving close interpersonal proximity. It was expected that subjects with far interpersonal distance preferences would experience greater stress-related arousal in a crowded environment than subjects preferring closer interpersonal distances and consequently would display more physiological, behavioral, and subjective indications of discomfort. During the crowding experience, subjects preferring far interpersonal distances were found to be most physiologically stressed (as measured by electrodermal responses) and later reported having experienced greater somatic stress. Crowded subjects had higher skin conductance levels during exposure, thereafter reported greater discomfort due to restricted space, and manifested lower performance levels on creativity tasks administered immediately following crowding. Results are discussed in the context of the loss of established control processes and the breakdown of interaction-regulation mechanisms.},
  Keywords                 = {psychology, personal space}
}

