@Article{punzo-2015,
  author   = {V. {Punzo} and M. {Montanino} and B. {Ciuffo}},
  title    = {Do We Really Need to Calibrate All the Parameters? Variance-Based Sensitivity Analysis to Simplify Microscopic Traffic Flow Models},
