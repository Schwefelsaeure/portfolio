}

@Article{gear-2002,
  author   = {C.W. Gear and Ioannis G. Kevrekidis and Constantinos Theodoropoulos},
  title    = {'Coarse' integration/bifurcation analysis via microscopic simulators: micro-Galerkin methods},
  journal  = {Computers \& Chemical Engineering},
  year     = {2002},
  volume   = {26},
  number   = {7--8},
  pages    = {941--963},
  issn     = {0098--1354},
  doi      = {http://dx.doi.org/10.1016/S0098-1354(02)00020-0},
  abstract = {We present a time-stepper based approach to the 'coarse' integration and stability/bifurcation analysis of distributed reacting system models. The methods we discuss are applicable to systems for which the traditional modeling approach through macroscopic evolution equations (usually partial differential equations, PDEs) is not possible because the \{PDEs\} are not available in closed form. If an alternative, microscopic (e.g. Monte Carlo or Lattice Boltzmann) description of the physics is available, we illustrate how this microscopic simulator can be enabled (through a computational superstructure) to perform certain integration and numerical bifurcation analysis tasks directly at the coarse, systems level. This approach, when successful, can circumvent the derivation of accurate, closed form, macroscopic \{PDE\} descriptions of the system. The direct 'systems level' analysis of microscopic process models, facilitated through such numerical enabling technologies, may, if practical, advance our understanding and use of nonequilibrium systems.},
  comment  = {<note author="fd"> Very good, deep, step by step explanation of equation-free modeling. Contains original notation and terminology (lift, restrict, ...). Includes review of "traditional" techniques for analyzing a system. </note>},
  file     = {gear-2002.pdf:Articles\\gear-2002.pdf:PDF},
  keywords = {equation-free, mathematics, numerics, differential equations, modeling},
}

@Article{gear-2005,
  Title                    = {Projecting to a Slow Manifold: Singularly Perturbed Systems and Legacy Codes},
  Author                   = {Gear, C. W. and Kaper, T. J. and Kevrekidis, I. G. and Zagaris, A.},
  Journal                  = {SIAM Journal on Applied Dynamical Systems},
  Year                     = {2005},

  Month                    = {1},
  Number                   = {3},
  Pages                    = {711--732},
  Volume                   = {4},

  Abstract                 = {We consider dynamical systems possessing an attracting, invariant "slow manifold" that can be parameterized by a few observable variables. We present a procedure that, given a process for integrating the system step by step and a set of values of the observables, finds the values of the remaining system variables such that the state is close to the slow manifold to some desired accuracy. It should be noted that this is not equivalent to "integrating down to the manifold" since the latter process may significantly change the values of the observables. We consider problems whose solution has a singular perturbation expansion, although we do not know what it is nor have any way to compute it (because the system is not necessarily expressed in a singular perturbation form). We show in this paper that, under some conditions, computing the values of the remaining variables so that their (m + 1)st time derivatives are zero provides an estimate of the unknown variables that is an mth-order approximation to a point on the slow manifold in a sense to be defined. We then show how this criterion can be applied approximately when the system is defined by a legacy code rather than directly through closed form equations. This procedure can be valuable when one wishes to start a simulation of the detailed model on the slow manifold with particular values of observable variables characterizing the slow manifold.},
  Doi                      = {10.1137/040608295},
  File                     = {gear-2005.pdf:Articles\\gear-2005.pdf:PDF},
  ISSN                     = {1536-0040},
  Keywords                 = {mathematics, equation-free, system analysis},
  Publisher                = {Society for Industrial \& Applied Mathematics (SIAM)}
}

@Article{gear-2003,
  author    = {Gear, C. W. and Kevrekidis, Ioannis G.},
  title     = {Projective Methods for Stiff Differential Equations: Problems with Gaps in Their Eigenvalue Spectrum},
  journal   = {SIAM Journal on Scientific Computing},
  year      = {2003},
  volume    = {24},
  number    = {4},
  month     = {1},
  pages     = {1091--1106},
  doi       = {10.1137/S1064827501388157},
  abstract  = {We show that there exist classes of explicit numerical integration methods that can handle very stiff problems if the eigenvalues are separated into two clusters, one containing the "stiff", or fast components, and one containing the slow components. These methods have large average step sizes relative to the fast components. Conventional implicit methods involve the solution of non-linear equations at each step, which for large problems requires significant communication between processors on a multiprocessor machine. For such problems the methods proposed here have significant potential for speed improvement.},
