@Article{tordeux-2015,
  Title                    = {Quantitative comparison of estimations for the density within pedestrian streams},
  Author                   = {Antoine Tordeux and Jun Zhang and Bernhard Steffen and Armin Seyfried},
  Journal                  = {Journal of Statistical Mechanics: Theory and Experiment},
  Year                     = {2015},
