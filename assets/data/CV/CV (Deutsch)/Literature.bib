@Article{chraibi-2015,
  Title                    = {Assessment of Models for Pedestrian Dynamics with Functional Principal Component Analysis},
  Author                   = {M. Chraibi and T. Ensslen, H. Gottschalk and M. Saadi and A. Seyfried},
