  Author                   = {A. Garcimart\'{i}n and I. Zuriguel and J. M. Pastor and C. Mart\'{i}n-G\'{o}mez and D. R. Parisi},
  Booktitle                = {The Conference in Pedestrian and Evacuation Dynamics 2014},
