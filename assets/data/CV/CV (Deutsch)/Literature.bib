@Conference{yeh-2008,
  author     = {Yeh, H. and Curtis, S. and Patil, S. and van den Berg J. and Manocha, D. and Lin, M.},
  title      = {Composite Agents},
  booktitle  = {ACM SIGGRAPH Symposium on Computer Animation},
