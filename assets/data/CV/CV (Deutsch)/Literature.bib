@InCollection{green-2009,
  author    = {Nikos Green and Hauke R. Heekeren},
  booktitle = {Mind and Motion: The Bidirectional Link between Thought and Action},
  title     = {Perceptual decision making: a bidirectional link between mind and motion},
  doi       = {10.1016/S0079-6123(09)01317-X},
