}

@Article{mann-2012,
  author    = {Mann, Richard P. and Perna, Andrea and Str\"ombom, Daniel and Garnett, Roman and Herbert-Read, James E. and Sumpter, David J. T. and Ward, Ashley J. W.},
  title     = {Multi-scale Inference of Interaction Rules in Animal Groups Using Bayesian Model Selection},
