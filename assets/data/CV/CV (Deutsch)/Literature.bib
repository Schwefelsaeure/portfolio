@Article{zanlungo-2012,
  Title                    = {A Microscopic ``Social Norm'' Model to Obtain Realistic Macroscopic Velocity and Density Pedestrian Distributions},
  Author                   = {Zanlungo, Francesco and Ikeda, Tetsushi and Kanda, Takayuki},
  Journal                  = {PLoS ONE},
