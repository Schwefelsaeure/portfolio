@Article{worchel-1976,
  author   = {Stephen Worchel and Charles Teddie},
  title    = {The experience of crowding: A two-factor theory.},
  journal  = {Journal of Personality and Social Psychology},
  year     = {1976},
  volume   = {34},
  number   = {1},
  month    = {7},
  pages    = {30--40},
  doi      = {10.1037/0022-3514.34.1.30},
  abstract = {Proposed that the experience of crowding occurs in a 2-step process: (a) the individual becomes aroused by violations of personal space and (b) then attributes the cause of this arousal to other people in the environment. It was predicted that violation of personal space rather than simple density is the spatial variable associated with crowding and that the experience of crowding can be alleviated if the individual is distracted from making the attribution that his or her arousal is caused by other people. To test these predictions, interaction distance (close and far) and density (high and low) were varied separately in a study with 315 male undergraduates. For half of the conditions, pictures (attribution inhibitors) were placed on the walls of the experimental room, and in the other half, the walls were bare. Ss worked on 2 tasks and were then questioned about their experience of crowding. Interaction distance was more closely related to crowding than density, and pictures reduced the experiences of crowding only in the close interaction conditions. Results support the attribution analysis of crowding. (27 ref) (PsycINFO Database Record (c) 2012 APA, all rights reserved)},
  comment  = {<note author=gk> Quoted by John in novelli-2013. </note>},
  keywords = {psychology, personal space},
}

