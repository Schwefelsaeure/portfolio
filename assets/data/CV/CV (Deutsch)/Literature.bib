@Article{appert-rolland-2011,
  Title                    = {Frozen shuffle update for an asymmetric exclusion process on a ring},
  Author                   = {C. Appert-Rolland and J. Cividini and H. J. Hilhorst},
  Journal                  = {Journal of Statistical Mechanics: Theory and Experiment},
  Year                     = {2011},
  Number                   = {7},
  Pages                    = {P07009},
  Volume                   = {2011},

  Abstract                 = {We introduce a new rule of motion for a totally asymmetric exclusion process (TASEP) representing pedestrian traffic on a lattice. Its characteristic feature is that the positions of the pedestrians, modeled as hard-core particles, are updated in a fixed predefined order, determined by a phase attached to each of them. We investigate this model analytically and by Monte Carlo simulation on a one-dimensional lattice with periodic boundary conditions. At a critical value of the particle density a transition occurs from a phase with 'free flow' to one with 'jammed flow'. We are able to analytically predict the current-density diagram for the infinite system and to find the scaling function that describes the finite size rounding at the transition point.},
  Doi                      = {10.1088/1742-5468/2011/07/P07009},
  File                     = {appert-rolland-2011.pdf:Articles\\appert-rolland-2011.pdf:PDF}
}

@Article{appert-rolland-2011b,
  Title                    = {Frozen shuffle update for a deterministic totally asymmetric simple exclusion process with open boundaries},
  Author                   = {C. Appert-Rolland and J. Cividini and H. J. Hilhorst},
  Journal                  = {Journal of Statistical Mechanics: Theory and Experiment},
  Year                     = {2011},
  Number                   = {10},
  Pages                    = {P10013},
  Volume                   = {2011},

  Abstract                 = {We introduce a new update algorithm for exclusion processes. Hard-core particles hop in a single direction on a discrete lattice and are updated in a fixed order, determined by a phase attached to each particle. The bulk dynamics is completely deterministic. While the case of periodic boundary conditions was studied in a companion paper, we consider here open boundaries through which particles enter and exit at random times. The full phase diagram is predicted analytically and exhibits a transition between a free flow phase and a jammed phase. The density profile is predicted within the framework of a domain wall theory and compared to Monte Carlo simulations, in particular in the vicinity of the transition. We discuss the relevance of the algorithm for real pedestrian traffic.},
  Doi                      = {10.1088/1742-5468/2011/10/P10013},
  File                     = {appert-rolland-2011b.pdf:Articles\\appert-rolland-2011b.pdf:PDF}
}

@Article{appert-rolland-2011c,
  Title                    = {Intersection of two TASEP traffic lanes with frozen shuffle update},
  Author                   = {C. Appert-Rolland and J. Cividini and H. J. Hilhorst},
  Journal                  = {Journal of Statistical Mechanics: Theory and Experiment},
  Year                     = {2011},
  Number                   = {10},
  Pages                    = {P10014},
  Volume                   = {2011},

  Abstract                 = {Motivated by interest in pedestrian traffic we study two lanes (one-dimensional lattices) of length L that intersect at a single site. Each lane is modeled by a deterministic TASEP (totally asymmetric exclusion process). The particles enter and leave lane \sigma (where \sigma = 1, 2) with probabilities \alpha \sigma and \beta \sigma , respectively. We employ the 'frozen shuffle' update introduced in earlier work (Appert-Rolland et al 2011 J. Stat. Mech. P07009), in which the particle positions are updated in a fixed random order. We find analytically that each lane may be in a 'free flow' or in a 'jammed' state. Hence the phase diagram in the domain 0 \leq \alpha 1, \alpha 2 \leq 1 consists of four regions with boundaries depending on \beta 1 and \beta 2 . The regions meet in a single point on the diagonal of the domain. Our analytical predictions for the phase boundaries as well as for the currents and densities in each phase are confirmed by Monte Carlo simulations.},
  Doi                      = {10.1088/1742-5468/2011/10/P10014},
  File                     = {appert-rolland-2011c.pdf:Articles\\appert-rolland-2011c.pdf:PDF}
}

@InProceedings{appert-rolland-2014,
  author    = {Appert-Rolland, C. and Jelic, A. and Degond, P. and Fehrenbach, J. and Hua, J. and Cretual, A. and Kulpa, R. and Marin, A. and Olivier, A.-H. and Lemercier, S. and Pettre, J.},
  booktitle = {Pedestrian and Evacuation Dynamics 2012},
  title     = {Experimental Study of the Following Dynamics of Pedestrians},
  doi       = {10.1007/978-3-319-02447-9_25},
  editor    = {Weidmann, Ulrich and Kirsch, Uwe and Schreckenberg, Michael},
  pages     = {305--315},
  publisher = {Springer International Publishing},
  abstract  = {We report some experimental study of the behavior of pedestrians when they follow each other. In the frame of the PEDIGREE project, trajectories of pedestrians walking along a one-dimensional path were tracked through a high-precision motion capture. Data analysis allowed to obtain the fundamental diagram at different scales. Two unexpected transitions in the way pedestrians follow each other have been evidenced. The interest of the experiment is to capture at the same time microscopic and macroscopic characteristics of the flow. Indeed, macroscopic structures such as stop-and-go waves can also be studied from the data. Eventually, a data-based following model has been proposed. Its calibration/validation can be performed both at the microscopic or macroscopic level. It is possible to extend the model to quasi-one-dimensional flows for the modeling of pedestrian flows in corridors.},
  file      = {appert-rolland-2014.pdf:Conference\\appert-rolland-2014.pdf:PDF},
  year      = {2014},
}

