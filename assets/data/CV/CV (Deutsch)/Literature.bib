@Article{qiang-2014,
  author   = {Sheng-Jie Qiang and Bin Jia and Dong-Fan Xie and Zi-You Gao},
  title    = {Reducing airplane boarding time by accounting for passengers' individual properties: A simulation based on cellular automaton},
