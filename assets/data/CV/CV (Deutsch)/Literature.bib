@article{muramatsu-1999,
  title = {Jamming transition in pedestrian counter flow},
  author = {Muramatsu, Masakuni and Irie, Tunemasa and Nagatani, Takashi},
  journal = {Physica A: Statistical Mechanics and its Applications},
  volume = {267},
  number = {3},
  pages = {487--498},
